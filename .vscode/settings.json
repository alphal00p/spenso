{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "rust-analyzer.cargo.features": "all",
  "conventionalCommits.scopes": [
    "iterators",
    "test",
    "CI",
    "network",
    "contraction",
    "symbolica",
    "clippy",
    "build",
    "gammaloop"
  ],
  "github.copilot.enable": {
    "quarto": false,
    "*": true
  },
  "rust-analyzer.showUnlinkedFileNotification": false
}
